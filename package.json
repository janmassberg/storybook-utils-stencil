{
  "name": "@jmassberg/storybook-utils",
  "version": "0.1.0",
  "description": "Utilities, code generators and components for storybook based library documentation",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "index.js",
    "index.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/janmassberg/storybook-utils-stencil.git"
  },
  "scripts": {
    "start": "npm run storybook",
    "clean": "rm -rf dist out coverage",
    "prebuild": "npm run clean",
    "build": "rollup -c rollup.config.ts --environment BUILD:prod",
    "build-storybook": "storybook build",
    "storybook": "storybook dev -p 6006 --debug-webpack",
    "eslint": "eslint src/**/*{.ts,.tsx}",
    "eslint:fix": "npm run eslint -- --fix",
    "prettier": "prettier \"./{*.js,*.ts,src/**/*{.scss,.ts,.tsx},stories/**/*{.mdx,.js,.ts}}\"",
    "prettier:check": "npm run prettier -- --check",
    "prettier:fix": "npm run prettier -- --write",
    "lint": "npm run eslint && npm run prettier",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "tslint": "tsc --noEmit",
    "test": "jest",
    "test:update": "jest -u",
    "test:report": "jest --json --outputFile ./jest.report.json --coverage"
  },
  "dependencies": {
    "prettier": "2.8.8"
  },
  "peerDependencies": {
    "@storybook/components": ">= 7.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.5",
    "@babel/core": "^7.24.5",
    "@babel/eslint-parser": "^7.24.5",
    "@babel/plugin-transform-runtime": "^7.24.3",
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-react": "^7.24.1",
    "@babel/preset-typescript": "^7.24.1",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-typescript": "^11.1.1",
    "@storybook/addon-actions": "^8.0.10",
    "@storybook/addon-essentials": "^8.0.10",
    "@storybook/addon-links": "^8.0.10",
    "@storybook/addon-mdx-gfm": "^8.0.10",
    "@storybook/addon-webpack5-compiler-babel": "^3.0.3",
    "@storybook/react": "^8.0.10",
    "@storybook/react-webpack5": "^8.0.10",
    "@types/jest": "^27.0.2",
    "@types/prettier": "^2.4.1",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "autoprefixer": "^10.3.7",
    "babel-loader": "^8.2.2",
    "eslint": "^7.32.0",
    "eslint-plugin-jest": "^25.2.1",
    "eslint-plugin-react": "^7.26.1",
    "jest": "^29.7.0",
    "postcss-flexbugs-fixes": "^5.0.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rollup": "^2.79.1",
    "storybook": "^8.0.10",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "typescript": "^4.8.2"
  }
}
